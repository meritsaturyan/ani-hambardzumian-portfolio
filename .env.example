# PostgreSQL connection URL
# Example:
# DATABASE_URL=postgres://user:password@localhost:5432/ani_studios
DATABASE_URL=postgres://USER:PASS@HOST:5432/DB_NAME

# If your hosting requires SSL for Postgres, set PGSSL=true
PGSSL=false

# Admin credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=supersecretpassword

# Secret for signing admin session tokens
ADMIN_SESSION_SECRET=change-me-to-a-long-random-string
